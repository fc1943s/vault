{
  "vimMode": true,
  "promptDelete": false,
  "attachmentFolderPath": "./file_cache",
  "newLinkFormat": "absolute",
  "newFileLocation": "current",
  "alwaysUpdateLinks": true,
  "useMarkdownLinks": true,
  "showUnsupportedFiles": true,
  "spellcheckLanguages": null,
  "livePreview": false,
  "showFrontmatter": false,
  "showLineNumber": true,
  "mobileToolbarCommands": [
    "editor:insert-wikilink",
    "editor:insert-embed",
    "editor:insert-tag",
    "editor:attach-file",
    "editor:set-heading",
    "editor:toggle-bold",
    "editor:toggle-italics",
    "editor:toggle-strikethrough",
    "editor:toggle-highlight",
    "editor:toggle-code",
    "editor:toggle-blockquote",
    "editor:insert-link",
    "editor:toggle-bullet-list",
    "editor:toggle-numbered-list",
    "editor:toggle-checklist-status",
    "editor:indent-list",
    "editor:unindent-list",
    "editor:undo",
    "editor:redo",
    "editor:toggle-keyboard",
    "editor:configure-toolbar",
    "editor:toggle-source",
    "command-palette:open",
    "editor:context-menu",
    "editor:paste",
    "editor:copy",
    "editor:cut",
    "editor:go-end",
    "editor:go-start",
    "obsidian-tasks-plugin:edit-task",
    "obsidian-tasks-plugin:toggle-done",
    "editor:open-link-in-new-window",
    "obsidian-git:view-file-on-github",
    "obsidian-git:view-history-on-github"
  ],
  "useTab": false,
  "userIgnoreFilters": [
    "dist/",
    "target/",
    "deps/"
  ]
}